        "illuminate/events": "^5.1",
        "gregwar/captcha": "^1.1",
        "firebase/php-jwt": "^4.0",
        "hashids/hashids": "^2.0",
        "zircote/swagger-php": "^2.0",
        "weaponhsu/ylaliopen": "^0.1.3",
        "weaponhsu/ylcodegenerator": "^0.4.6"
